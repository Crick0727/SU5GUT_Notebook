  doi = {10.1016/0370-2693(79)90811-6},
  urldate = {2025-01-26},
  abstract = {Rigorous bounds on Kl3 spectral functions with up to two substractions are used in order to test the hypothesis mu = 0 Results strongly support an earlier proof, based on a no-subtraction assumption, which shows that the non-renormalization theorem is badly violated if mu= 0. Also, a relation between the Kl3{\textpm} scalar form factor and the quark mass ratio mu/ms is obtained in the soft pion limit. Predictions from this relation are then discussed and compared with other estimates based on the baryon and meson spectra and the {$\eta\rightarrow$}3{$\pi$} decay.},
  file = {/home/yasutoki/Zotero/storage/X69EQEQ6/Dominguez - 1979 - Determination of quark mass ratios from Kll3 decay.pdf;/home/yasutoki/Zotero/storage/PP5V7EQ4/0370269379908116.html}
}

