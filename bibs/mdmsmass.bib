@article{langackerLightquarkMassSpectrum1979,
  title = {Light-Quark Mass Spectrum in Quantum Chromodynamics},
  author = {Langacker, Paul and Pagels, Heinz},
  year = {1979},
  month = apr,
  journal = {Physical Review D},
  volume = {19},
  number = {7},
  pages = {2070--2079},
  publisher = {American Physical Society},
  doi = {10.1103/PhysRevD.19.2070},
  urldate = {2025-01-26},
  abstract = {Quantum chromodynamics has placed the problem of hadronic symmetry breaking on a rational basis. The current-quark mass ratios can be shown to be renormalization-group invariants up to small and controllable corrections from flavor interactions. We calculate the mass ratios of the light {$u$}, {$d$}, and {$s$} quarks using the pseudoscalar-meson mass spectrum, the baryon mass spectrum, and the {$H$} {$\rightarrow$}3⁢{$\pi$} decay. The main theoretical assumptions are that low-lying-resonance and Born terms correctly estimate the photonic contribution to isotopic mass splitting and that chiral perturbation theory---equivalently kaon partial convervation of axialvector current---correctly estimates chiral-symmetry breaking. Taking account of all leading-order chiral corrections to the meson spectrum and from the baryon spectrum and {$H$} {$\rightarrow$}3⁢{$\pi$} decay we obtain {$mumd$} =0.38 {\textpm}0.13 and {$mdms$} =0.045 {\textpm}0.011. We conclude that while a vanishing up-quark mass is not rigorously ruled out it is unattractive from the standpoint of the presently consistent phenomenology of hadronic symmetry breaking.},
  file = {/home/yasutoki/Zotero/storage/38SQHSHE/Langacker and Pagels - 1979 - Light-quark mass spectrum in quantum chromodynamics.pdf;/home/yasutoki/Zotero/storage/RTFD9ZYT/PhysRevD.19.html}
}

